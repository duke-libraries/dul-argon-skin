<% if document.duke_requestable? %>
  <%= link_to I18n.t('trln_argon.show.request'), document.request_item_url,
    class: "btn btn-success-action request-from-duke", target: "_blank" %>
<% end %>

<% if document.trln_requestable? %>
  <%= link_to I18n.t('trln_argon.show.request'), document.illiad_request_url_default,
    class: "btn btn-success-action request-from-trln", target: "_blank",
    data: { toggle: 'modal', target: '#trln-request-modal',
      illiad_params: document.illiad_request_params, remote: false } %>
<% end %>
